{
  "expo": {
    "name": "facebookReactionsReactNative",
    "slug": "facebookReactionsReactNative",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.raahimkhan.facebookReactions",
      "config": {
        "usesNonExemptEncryption": false
      },
      "entitlements": {
        "aps-environment": "development"
      },
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.raahimkhan.facebookReactions",
      "versionCode": 1
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 30,
            "targetSdkVersion": 34,
            "extraProguardRules": "-keep class com.shopify.reactnative.skia.** { *; }"
          },
          "ios": {
            "deploymentTarget": "16.0"
          }
        }
      ]
    ],
    "owner": "raahimkhan_",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "32d273db-8799-4285-a12e-8ae08cedc20b"
      }
    }
  }
}
